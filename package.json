{
  "name": "@bedrockio/eslint-plugin",
  "version": "1.4.4",
  "description": "Common ESLint plugin for Bedrock projects.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "scripts/build",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./src/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/cjs/index.js"
  },
  "dependencies": {
    "@eslint/js": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-exports": "^1.0.0-beta.5",
    "eslint-import-resolver-vite": "^2.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.13.0",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@bedrockio/prettier-config": "^1.1.1",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "eslint-import-resolver-webpack": "^0.13.10",
    "typescript-eslint": "^8.55.0"
  },
  "peerDependenciesMeta": {
    "eslint-import-resolver-webpack": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  },
  "prettier": "@bedrockio/prettier-config",
  "volta": {
    "node": "24.13.1",
    "yarn": "1.22.22"
  }
}

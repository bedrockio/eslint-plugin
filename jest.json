{
  "extends": [
    "plugin:jest/recommended"
  ],
  "rules": {
    "jest/expect-expect": [
      "error",
      {
        "assertFunctionNames": ["expect*", "assert*"]
      }
    ],
    "jest/no-focused-tests": "off",
    "jest/no-disabled-tests": "off",
    "jest/no-conditional-expect": "off"
  }
}
